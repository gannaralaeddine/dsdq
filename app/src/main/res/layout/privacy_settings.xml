<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view11"
        android:layout_width="wrap_content"
        android:layout_height="87dp"
        android:background="#21CADD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView64"
        android:layout_width="112dp"
        android:layout_height="47dp"
        android:layout_marginStart="204dp"
        android:layout_marginLeft="204dp"
        android:text="Save"
        android:textColor="#FFFFFF"

        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view11"
        app:layout_constraintStart_toEndOf="@+id/button26"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button26"
        android:layout_width="49dp"
        android:layout_height="53dp"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:background="@drawable/abc_vector_test"
        app:layout_constraintBottom_toBottomOf="@+id/view11"
        app:layout_constraintStart_toStartOf="@+id/view11"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="349dp"
        android:layout_height="575dp"
        android:background="@drawable/style2"
        android:cardPreventCornerOverlap="false"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view11">

        <LinearLayout
            android:layout_width="337dp"
            android:layout_height="115dp"
            android:layout_marginLeft="10dp"
            android:orientation="vertical">

            <Switch
                android:id="@+id/switch1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:switchTextAppearance="@style/TextAppearance.AppCompat.Large"
                android:text="Booking"
                android:textColor="#717070"
                android:textSize="24sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView66"
                android:layout_width="279dp"
                android:layout_height="wrap_content"
                android:text="Make the appointment/bookings  formvisible on my public profile" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="338dp"
            android:layout_height="115dp"
            android:layout_marginLeft="10dp"
            android:orientation="vertical">

            <Switch
                android:id="@+id/switch2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:switchTextAppearance="@style/TextAppearance.AppCompat.Large"
                android:text="Phone Number"
                android:textColor="#717070"
                android:textSize="24sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView67"
                android:layout_width="279dp"
                android:layout_height="wrap_content"
                android:text="Make the phone visible on my  public profile" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="337dp"
            android:layout_height="115dp"
            android:layout_marginLeft="10dp"
            android:orientation="vertical">

            <Switch
                android:id="@+id/switch3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:switchTextAppearance="@style/TextAppearance.AppCompat.Large"
                android:text="Email"
                android:textColor="#717070"
                android:textSize="24sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView68"
                android:layout_width="279dp"
                android:layout_height="wrap_content"
                android:text="Make the email address visible  on my public profile" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="337dp"
            android:layout_height="115dp"
            android:layout_marginLeft="10dp"
            android:orientation="vertical">

            <Switch
                android:id="@+id/switch4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:switchTextAppearance="@style/TextAppearance.AppCompat.Large"
                android:text="Contact Form"
                android:textColor="#717070"
                android:textSize="24sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView69"
                android:layout_width="279dp"
                android:layout_height="wrap_content"
                android:text="Make the contact form visible  on my public profile" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="334dp"
            android:layout_height="115dp"
            android:layout_marginLeft="10dp"
            android:orientation="vertical">

            <Switch
                android:id="@+id/switch5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:switchTextAppearance="@style/TextAppearance.AppCompat.Large"
                android:text="Opening Hours"
                android:textColor="#717070"
                android:textSize="24sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView70"
                android:layout_width="279dp"
                android:layout_height="wrap_content"
                android:text="Show or hide opening hours. " />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>